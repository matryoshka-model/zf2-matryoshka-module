{
    "name": "matryoshka-model/zf2-matryoshka-module",
    "description": "ZF2 module for matryoshka library",
    "license": "BSD-2-Clause",
    "authors": [
        {
            "name": "Leonardo Grasso",
            "email": "me@leonardograsso.com",
            "homepage": "http://github.com/leogr"
        },
        {
            "name": "Leo Di Donato",
            "email": "leodidonato@gmail.com",
            "homepage": "http://github.com/leodido"
        },
        {
            "name": "Antonio Visalli",
            "email": "antonino.visalli@gmail.com",
            "homepage": "http://github.com/visa4"
        },
        {
            "name": "Lorenzo Fontana",
            "email": "fontanalorenzo@me.com",
            "homepage": "https://github.com/fntlnz"
        }
    ],
    "require": {
        "php": ">=5.4",
        "matryoshka-model/matryoshka": "0.5.*@dev",
        "zendframework/zend-mvc": "2.*",
        "zendframework/zend-modulemanager": "2.*"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.2",
        "satooshi/php-coveralls": "dev-master"
    },
    "autoload": {
        "psr-0": {
            "Matryoshka": "src/",
            "MatryoshkaTest": "tests/"
        },
        "classmap": [
            "./Module.php"
        ]
    },
    "keywords": [
        "model",
        "mvc",
        "matryoshka",
        "back-end",
        "db",
        "database",
        "zf2"
    ],
    "extra": {
        "branch-alias": {
            "dev-master": "0.4.x-dev",
            "dev-develop": "0.5.x-dev"
        }
    },
    "suggest": {
        "matryoshka-model/mongo-wrapper": "MongoDB matryoshka wrapper",
        "matryoshka-model/rest-wrapper": "Matryoshka wrapper aimed at creating restful APIs"
    }
}
