{
    "name": "matryoshka-model/zf2-matryoshka-module",
    "description": "ZF2 module for Matryoshka",
    "license": "BSD-2-Clause",
    "minimum-stability": "dev",
    "authors": [
        {
            "name": "Leonardo Grasso",
            "email": "me@leonardograsso.com"
        }
    ],
    "require": {
        "php": ">=5.4",
        "matryoshka-model/matryoshka": "~0.2"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*",
        "satooshi/php-coveralls": "dev-master",
        "fabpot/php-cs-fixer": "dev-master",
        "zendframework/zendframework": "2.*"
    },
    "autoload": {
        "psr-0": {
            "Matryoshka": "src/",
            "MatryoshkaTest": "tests/"
        },
        "classmap": [
            "./Module.php"
        ]
    }
}