{
  "name": "matryoshka-model/zf2-matryoshka-module",
  "description": "ZF2 integration module for Matryoshka",
  "license": "BSD-2-Clause",
  "authors": [
    {
      "name": "Leonardo Grasso",
      "email": "me@leonardograsso.com",
      "homepage": "http://github.com/leogr"
    },
    {
      "name": "Leo Di Donato",
      "email": "leodidonato@gmail.com",
      "homepage": "http://github.com/leodido"
    },
    {
      "name": "Antonio Visalli",
      "email": "antonino.visalli@gmail.com",
      "homepage": "http://github.com/visa4"
    },
    {
      "name": "Lorenzo Fontana",
      "email": "fontanalorenzo@me.com",
      "homepage": "https://github.com/fntlnz"
    }
  ],
  "require": {
    "php": ">=5.5",
    "matryoshka-model/matryoshka": "~0.8.0",
    "zendframework/zend-mvc": "2.*",
    "zendframework/zend-modulemanager": "2.*"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.3",
    "satooshi/php-coveralls": "dev-master"
  },
  "autoload": {
    "psr-4": {
      "Matryoshka\\Module\\": "src/",
      "MatryoshkaModuleTest\\": "tests/"
    },
    "classmap": [
      "./Module.php"
    ]
  },
  "keywords": [
    "model",
    "mvc",
    "matryoshka",
    "back-end",
    "backend",
    "db",
    "database",
    "zf2",
    "module"
  ],
  "extra": {
    "branch-alias": {
      "dev-master": "0.7.x-dev",
      "dev-develop": "0.8.x-dev"
    }
  },
  "suggest": {
    "matryoshka-model/mongo-wrapper": "MongoDB matryoshka wrapper",
    "matryoshka-model/rest-wrapper": "Matryoshka wrapper aimed at creating restful API clients",
    "matryoshka-model/service-api": "A set of utilities aimed at consuming HTTP API services"
  }
}
